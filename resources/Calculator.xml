<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Suite1">
	<test name="CalculatorTest" parallel="methods" thread-count="2" group-by-instances="true">		
		<groups>
			<run>
				<include name="positive" />
				<include name="negative" />
				<include name="tangent" />
				<include name="cosine" />
			</run>
		</groups>
		<classes>
			<class name="com.atm.xunit.hometask.runner.CalculatorTest" />
		</classes>
	</test>

</suite>
